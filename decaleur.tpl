// Get the required API and set the first vars
const createQueue = require('createQueue');
const logToConsole = require('logToConsole');
const copyFromDataLayer = require('copyFromDataLayer');
const setInWindow = require('setInWindow');
const copyFromWindow = require('copyFromWindow');

let page_view_event_name = data.page_view_event_name;
//logToConsole('info', 'useRegex', data.useRegExp);
var dataLayerPush = createQueue('dataLayer');
var dataSecondLayer = copyFromDataLayer('dataSecondLayer');

// Set window.pageViewDetected to false if it does not exist and log it's current value
setInWindow('pageViewDetected', false, false);
var pageViewStatus = copyFromWindow('pageViewDetected');

function flushQueue() {
  // This function does the following:
  //   1. fire each of the delayed events and its key:value params stored in the 2nd layer of the dataLayer (dataSecondLayer)
  //   2. after each event fired above it clears the used params from the main dataLayer to avoid reusing them being in subsequent event firing
  //   3. Remove the used params from the 2nd level of the dataLayer
  var dataSecondLayer = copyFromDataLayer('dataSecondLayer');

  // If the second Layer is empty, stop funtcion
  if (dataSecondLayer.length === 0) {
    return;
  }

  // For each object in the backlogged events in the 2nd layer.
  while (dataSecondLayer.length > 0) {
    // Pick the first one and remove it from the dataSecondLayer local var
    var eventItem = dataSecondLayer.shift();
    
    // If it's an object, rebuild it to match the new naming convention
    if (eventItem && typeof eventItem === 'object') {

      // Create the new object and the object used to clear the info moved to the 2nd dataLayer
      var formattedEventItem = {};
      var clearFormattedEventItem = {event:'clearingUsedParams'};
      formattedEventItem.eventName = eventItem.event; // Rename the event as eventName
      formattedEventItem.event = 'firingDelayedEvent'; // Add the new event and set its value as firingDelayedEvent
      
      // For each params, add its key:value pair to the new object and set the original as undefined to avoid the reuse of the params in later event firing
      for (var key in eventItem) {
        if (key !== 'event') {
          formattedEventItem[key] = eventItem[key];
          clearFormattedEventItem[key] = undefined; // Original params resetted as undefined
        }
      }
      // Push event to 2nd layer
      dataLayerPush(formattedEventItem);
      
      // Erase used information
      dataLayerPush(clearFormattedEventItem);
      
    } else {
      logToConsole('error', 'Invalid element detected in the queue :', eventItem);
    }
  }
  // Empty the dataSecondLayer since all has been fired
  dataLayerPush({event:'clearingUsedInfoArray', dataSecondLayer : undefined});
}

// If the event name is page_view, set the window.pageViewDetected to true and fire backlogged events
if (((!data.useRegExp && copyFromDataLayer('event') === page_view_event_name) || (data.useRegExp && copyFromDataLayer('event').match(page_view_event_name))) && pageViewStatus !== true) {
  setInWindow('pageViewDetected', true, true);
  if(copyFromDataLayer('dataSecondLayer') !== undefined){
    flushQueue();
  }
}

// Inform GTM that the tag successfully completed
data.gtmOnSuccess();


